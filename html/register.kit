<!DOCTYPE html>
<html lang="pl">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Ravish.pl | Logowanie</title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <meta name="robots" content="noindex" />
    <link
        href="https://fonts.googleapis.com/css2?family=Courier+Prime:ital,wght@0,400;0,700;1,400;1,700&family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap"
        rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Bebas+Neue&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="dist/css/style.min.css">
</head>

<body>
    <!-- @Include 'nav' -->
    <!-- @include 'notification' -->
    <section class="login">
        <div class="wrapper">
            <h1 class="login__title">Ravish.pl | Rejestracja</h1>
            <div class="login-box">
                <div class="login-left login-item">
                    <h2 class="login__titles">Zarejestruj się</h2>
                    <div class="login__line"></div>
                    <div class="login-names">
                        <input type="text" class="login-names__item " placeholder="*Imię">
                        <input type="text" class="login-names__item " placeholder="*Nazwisko">
                    </div>
                    <input type="email" class="login-item login-left__input" placeholder="*Email">
                    <input type="password" class="login-left__input" placeholder="*Hasło">
                    <input type="date" class="login-left__input">
                    <select name="gender" class="login-left__input">
                        <option value="1" class="login-select__item">Twoja płeć</option>
                        <option value="2" class="login-select__item">Mężczyzna</option>
                        <option value="3" class="login-select__item">Kobieta</option>
                        <option value="4" class="login-select__item">Nie chce podawać</option>
                    </select>
                </div>
            </div>
        </div>
    </section>
    <!-- @Include 'footer' -->
    <script src="dist/js/nav.min.js"></script>
</body>

</html>