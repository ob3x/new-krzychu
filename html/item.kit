                    <th class="popup-item__head-tr--item">Rozmiar</th>
                    <th class="popup-item__head-tr--item">Szerokość</th>
                    <th class="popup-item__head-tr--item">Długość</th>
                    <th class="popup-item__head-tr--item">Pas</th>
                </tr>
            </thead>
            <tbody>
                <tr class="popup-item__box">
                    <td class="popup-item__head-tr--item">S</td>
                    <td class="popup-item__box-color">50 - 54</td>
                    <td>70 - 12</td>
                    <td class="popup-item__box-color">80 - 12</td>
                </tr>
                <tr class="popup-item__box">
                    <td class="popup-item__head-tr--item">M</td>
                    <td class="popup-item__box-color">55 - 60</td>
                    <td>75 - 12</td>
                    <td class="popup-item__box-color">85 - 12</td>
                </tr>
                <tr class="popup-item__box">
                    <td class="popup-item__head-tr--item">L</td>
                    <td class="popup-item__box-color">60 - 12</td>
                    <td>80 - 12</td>
                    <td class="popup-item__box-color">90 - 12</td>
                </tr>
                <tr class="popup-item__box">
                    <td class="popup-item__head-tr--item">XL</td>
                    <td class="popup-item__box-color">65 - 12</td>
                    <td>85 - 12</td>
                    <td class="popup-item__box-color">95 - 12</td>
                </tr>
            </tbody>
        </table>
