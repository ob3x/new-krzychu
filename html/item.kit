    <section class="item">
        <div class="item-box">
            <div class="item-left">
                <div class="item-left__box">
                    <img src="dist/img/united-2.png" alt="koszulka" class="item-left__box--img">
                    <img src="dist/img/Untitled-1.png" alt="koszulka" class="item-left__box--img">
                    <img src="dist/img/united-2.png" alt="koszulka" class="item-left__box--img">
                    <img src="dist/img/Untitled-1.png" alt="koszulka" class="item-left__box--img">
